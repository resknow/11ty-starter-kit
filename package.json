{
	"name": "11ty-starter",
	"version": "2.0.0",
	"description": "An SEO ready starter kit for 11ty with Design Tokens, Responsive Type Scale and Cypress Testing.",
	"main": ".eleventy.js",
	"scripts": {
		"build": "NODE_ENV=production npm-run-all eleventy:build",
		"dev": "NODE_ENV=development npm-run-all eleventy:serve",
		"demo": "NODE_ENV=development DEMO_MODE=true npm-run-all eleventy:serve",
		"eleventy:serve": "eleventy --serve",
		"eleventy:build": "eleventy",
		"test": "npm-run-all --parallel dev cypress",
		"cypress": "npx cypress open"
	},
	"author": "Chris Galbraith",
	"license": "MIT",
	"devDependencies": {
		"@11ty/eleventy": "^0.11.0",
		"@11ty/eleventy-navigation": "^0.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^3.1.1",
		"chalk": "^3.0.0",
		"clean-css": "^5.1.2",
		"cypress": "^3.8.0",
		"esbuild": "^0.12.9",
		"globby": "^5.0.0",
		"node-sass-glob-importer": "^5.3.2",
		"npm-run-all": "^4.1.5",
		"sass": "^1.34.1",
		"unique-string": "^3.0.0"
	}
}
